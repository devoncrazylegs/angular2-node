{
  "name": "piglatin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"tsc -w --p client/tsconfig.json\" \"nodemon ./bin/www\"",
    "startserver": "nodemon ./bin/www",
    "tsc": "tsc --p client/tsconfig.json",
    "tsc:w": "tsc -w --p client/tsconfig.json",
    "build": "gulp build --gulpfile client/gulpfile.js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "redis": "^2.6.5",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "typings": "^2.1.0",
    "concurrently": "^2.2.0"
  }
}
